{
  "name": "highspot",
  "version": "1.0.0",
  "description": "",
  "main": ".eslintrc.js",
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint",
    "dev": "cross-env TS_NODE_PROJECT=./tsconfig.webpack.json NODE_ENV=development webpack-dev-server --hot"
  },
  "jest": {
    "transform": {
      "\\.[tj]sx?$": "babel-jest"
    },
    "testRegex": "\\.test\\.tsx?$",
    "setupFilesAfterEnv": [
      "./src/opt/setupTest.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "^~elements(.*)$": "<rootDir>/src/elements$1",
      "^~etc(.*)$": "<rootDir>/src/etc$1",
      "^~opt(.*)$": "<rootDir>/src/opt$1",
      "^~util(.*)$": "<rootDir>/src/util$1",
      "^~data(.*)$": "<rootDir>/src/data$1",
      "^~(.*)$": "<rootDir>/src$1"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@emotion/babel-preset-css-prop": "^10.0.9",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/jest": "^24.0.13",
    "@types/webpack": "^4.4.32",
    "@types/webpack-dev-server": "^3.1.6",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-emotion": "^10.0.9",
    "babel-plugin-lodash": "^3.3.4",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-typescript": "^4.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "react-testing-library": "^7.0.1",
    "sanitize.css": "^9.0.0",
    "style-loader": "^0.23.1",
    "ts-node": "^8.2.0",
    "typeface-roboto": "0.0.54",
    "typescript": "^3.4.5",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@emotion/core": "^10.0.10",
    "@emotion/styled": "^10.0.11",
    "@hot-loader/react-dom": "^16.8.6",
    "@types/axios": "^0.14.0",
    "@types/lodash": "^4.14.132",
    "@types/react": "^16.8.18",
    "@types/react-dom": "^16.8.4",
    "@types/react-hot-loader": "^4.1.0",
    "@types/react-redux": "^7.0.9",
    "@types/react-router-dom": "^4.3.3",
    "@types/redux-devtools-extension": "^2.13.2",
    "@types/uuid": "^3.4.4",
    "axios": "^0.18.1",
    "lodash": "^4.17.11",
    "mtgsdk": "^0.4.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.8.8",
    "react-redux": "^7.1.0-alpha.5",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^1.0.2",
    "redux-ts-utils": "^3.2.2",
    "uuid": "^3.3.2"
  }
}
